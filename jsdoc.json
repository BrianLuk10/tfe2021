{
    "source":{
        "include": ["src"],
        "includePattern": ".js$",
        "excludePattern": "(nodes_modules/|document)"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "cleverLinks":true,
        "monospaceLinks":true
    },
    "opts": {
        "recurse":true,
        "destination": "./document/"
    }
}